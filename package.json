{
  "name": "@bitblit/ratchet",
  "version": "0.0.0",
  "description": "Common tools for general use",
  "private": true,
  "workspaces": {
    "packages": [
      "modules/*"
    ],
    "_nohoist": [
      "**/json5",
      "**/json5/**",
      "**/canvas",
      "**/canvas/**"
    ]
  },
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "William Weiss <npm@codification.org>",
    "Austin Grantham <agrantham@algidious.com>",
    "Lucas Myers <veganbeef@protonmail.com>",
    "Joel Flint <joel@stimulimedia.net>",
    "Bilal Shahid <bshahid@adomni.com>"
  ],
  "scripts": {
    "clean": "wsrun mod:clean",
    "build": "wsrun --report  --serial --fast-exit --exclude @bitblit/ngx-acute-common --exclude @bitblit/ngx-acute-warden mod:build && echo Building Angular && yarn build-ng && echo Completed full build",
    "build-ng": "wsrun --report --serial --fast-exit --package @bitblit/ngx-acute-common --package @bitblit/ngx-acute-warden -c mod:ng-build",
    "build-rollups": "wsrun --report --serial --fast-exit --package @bitblit/ngx-acute-common -c mod:rollup-build",
    "lint": "wsrun --serial mod:lint",
    "lint-fix": "wsrun --serial mod:lint-fix",
    "test": "echo Starting full test && wsrun --fast-exit mod:test",
    "test-coverage": "wsrun mod:test-coverage",
    "apply-github-info": "./apply-github-info.sh",
    "set-codependencies": "echo $GITHUB_RUN_NUMBER && ./set-codependencies.sh 5.2.$GITHUB_RUN_NUMBER",
    "set-codependencies-by-github-tag": "echo $GITHUB_RUN_NUMBER && ./set-codependencies-by-github-tag.sh 6.0.$GITHUB_RUN_NUMBER",
    "publish": "wsrun mod:publish",
    "mod:raw-tsc": "cd $INIT_CWD && tsc",
    "mod:generate-barrels": "cd $INIT_CWD && barrelsby -e \".*\\.spec\\.ts\" --delete -d src -l below -q && cd $INIT_CWD && fix-esm-import-path src && rm -f src/brevo/index.ts",
    "mod:build": "cd $INIT_CWD && yarn mod:clean && cd $INIT_CWD && tsc",
    "mod:ng-build": "cd $INIT_CWD && yarn mod:clean && cd $INIT_CWD && npx ngc",
    "mod:rollup-build": "cd $INIT_CWD && yarn mod:clean && cd $INIT_CWD && rollup -c ./rollup.config.mjs",
    "_mod:build": "cd $INIT_CWD && yarn mod:clean && yarn mod:generate-barrels && cd $INIT_CWD && tsc",
    "_rollup_mod:build": "cd $INIT_CWD && yarn mod:clean && yarn mod:generate-barrels && cd $INIT_CWD && rollup -c ../../rollup.global.config.mjs && echo Finished build",
    "mod:clean": "cd $INIT_CWD && shx rm -Rf lib",
    "mod:docs": "cd $INIT_CWD && typedoc",
    "mod:pretty": "cd $INIT_CWD && prettier src --write",
    "mod:lint": "cd $INIT_CWD && eslint -c ../../eslint.config.mjs src/**/*.ts",
    "mod:lint-fix": "cd $INIT_CWD && yarn mod:pretty && cd $INIT_CWD && eslint --fix -c ../../eslint.config.mjs src/**/*.ts",
    "mod:test": "cd $INIT_CWD && vitest run",
    "mod:test-coverage": "cd $INIT_CWD && vitest run  --coverage",
    "mod:watch": "cd $INIT_CWD && tsc-watch",
    "mod:force-build": "cd $INIT_CWD && tsc --build --force",
    "mod:publish": "cd $INIT_CWD && yarn npm publish --access public --provenance --tag $GITHUB_REF_NAME"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitblit/Ratchet.git"
  },
  "keywords": [
    "ratchet",
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=14.18"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "cross-fetch": "4.1.0",
    "luxon": "3.7.2"
  },
  "peerDependencies": {
    "cross-fetch": "^4.1.0",
    "luxon": "^3.7.2"
  },
  "resolutions": {
    "@babel/traverse": "^7.23.2",
    "@smithy/types": "^2.3.3",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cookie": "^0.7.0",
    "cross-spawn": "^7.0.6",
    "express": "^4.20.0",
    "micromatch": "^4.0.8",
    "nanoid": "^3.3.8",
    "nth-check": "^2.0.1",
    "semver": "^7.5.2",
    "path-to-regexp": "^0.1.12",
    "rollup": "^4.22.4",
    "sanitize-html": "^2.12.1",
    "send": "^0.19.0",
    "serve-static": "^1.16.0",
    "underscore": "^1.13.7",
    "undici": "^6.21.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "2.0.1",
    "@angular/compiler": "20.3.9",
    "@angular/compiler-cli": "20.3.9",
    "@eslint/js": "9.39.1",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.0",
    "@vitest/coverage-istanbul": "4.0.6",
    "@vitest/coverage-v8": "4.0.6",
    "barrelsby": "2.8.1",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "fix-esm-import-path": "1.10.3",
    "husky": "9.1.7",
    "node-gyp": "11.5.0",
    "npm-dts": "1.3.13",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "renamer": "5.0.2",
    "rimraf": "6.1.0",
    "rollup": "4.52.5",
    "rollup-plugin-angular": "0.5.3",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-natives": "0.7.8",
    "rollup-plugin-polyfill-node": "0.13.0",
    "rollup-plugin-typescript2": "0.36.0",
    "shx": "0.4.0",
    "ts-node": "10.9.2",
    "tsc-watch": "7.2.0",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3",
    "vitest": "4.0.6",
    "vitest-mock-extended": "3.1.0",
    "wsrun": "5.2.4"
  },
  "packageManager": "yarn@4.10.3"
}
