{
  "name": "@bitblit/ratchet-epsilon-common",
  "version": "0.0.0-snapshot",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "sideEffects": false,
  "bin": {
    "epsilon-cli": "./bin/cli.js"
  },
  "type": "module",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**",
    "bin/**"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs"
    }
  },
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "Benn Linger <benn@bennlinger.com>",
    "Lucas Myers <veganbeef@protonmail.com>",
    "Zach Herridge"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "run-local-container-server": "yarn build && node bin/cli.js run-local-container-server",
    "run-build-sample-server": "yarn build && node bin/cli.js run-sample-local-server",
    "run-sample-server": "node bin/cli.js run-sample-local-server",
    "run-test-error-server": "yarn build && node bin/cli.js run-test-error-server",
    "run-batch-sample-server": "yarn build && node bin/cli.js run-sample-local-batch-server",
    "run-background-process-from-command-line-dry-run": "yarn build && node bin/cli.js run-background-process"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Ratchet"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=14.18"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/abort-controller": "3.370.0",
    "@aws-sdk/client-s3": "3.370.0",
    "@aws-sdk/client-sns": "3.370.0",
    "@aws-sdk/client-sqs": "3.370.0",
    "@aws-sdk/types": "3.370.0",
    "@aws-sdk/util-waiter": "3.370.0",
    "@bitblit/ratchet-aws": "0.0.0-snapshot",
    "@bitblit/ratchet-common": "0.0.0-snapshot",
    "@bitblit/ratchet-misc": "0.0.0-snapshot",
    "@bitblit/ratchet-node-only": "0.0.0-snapshot",
    "clear": "0.1.0",
    "commander": "11.0.0",
    "cross-fetch": "4.0.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "3.0.1",
    "luxon": "3.3.0",
    "reflect-metadata": "0.1.13",
    "route-parser": "0.0.5",
    "rxjs": "7.8.1",
    "strip-js": "1.2.0",
    "swagger-model-validator": "3.0.21"
  },
  "peerDependencies": {
    "@apollo/server": "^4.7.5",
    "graphql": "^16.7.1"
  },
  "peerDependenciesMeta": {
    "@apollo/server": {
      "optional": true
    },
    "graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/server": "4.8.0",
    "@bitblit/ratchet-jest": "0.0.0-snapshot",
    "@types/aws-lambda": "8.10.119",
    "aws-sdk-client-mock": "3.0.0",
    "graphql": "16.7.1",
    "graphql-tag": "2.12.6"
  }
}
