    },
    "@smithy/smithy-client": {
      "optional": true
    },
    "@smithy/util-waiter": {
      "optional": true
    },
    "mailparser": {
      "optional": true
    },
    "mime-types": {
      "optional": true
    },
    "tmp": {
      "optional": true
    },
    "unzipper": {
      "optional": true
    },
    "walk": {
      "optional": true
    }
  }
}
